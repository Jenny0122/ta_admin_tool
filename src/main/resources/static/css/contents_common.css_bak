/* project toggle button*/
.task_enabled label {
	display: block;
	position: relative;
	width: 80px;
	height: 30px;
	background: #d3d3d3;
	border-radius: 60px;
	transition: background 0.4s;
}

.task_enabled label::before {
	content: "미사용";
	font-size: 14px;
	font-weight: 600;
	font-family: Arial, Helvetica, sans-serif;
	position: absolute;
	left: 32px;
	top: 50%;
	transform: translateY(-50%);
	transition: all 0.4s;
}

.task_enabled label::after {
	content: "";
	position: absolute;
	left: 2.5px;
	top: 50%;
	width: 20px;
	height: 20px;
	border-radius: 100%;
	background-color: #fff;
	transform: translateY(-50%);
	box-shadow: 1px 3px 4px rgba(0,0,0,0.1);
	transition: all 0.4s;
}

.task_enabled input[type="checkbox"]:checked + label {
	background:#31cc31;
}

.task_enabled input[type="checkbox"]:checked + label::before {
	content: "사용";
	color: #fff;
	left: 8px;
}

.task_enabled input[type="checkbox"]:checked + label::after {
	left: calc(100% - 25.5px);
}


/* button */
.btn{display:inline-block;box-sizing: border-box;height: 35px;line-height: 35px;border-radius: 3px;text-align: center; cursor:pointer; outline: none}
.radiusBtn{display:inline-block;box-sizing: border-box;height: 35px;line-height: 35px;border-radius: 10px;text-align: center; cursor:pointer; outline: none; font-weight:600;}
.btn_sky{background: #0054b3; color:#fff !important;}
.btn_blue{background: #0054b3;color:#fff !important;}
.btn_lightgray{background:#f5f5f5;color:#999 !important;}
.btn_gray{background:#f5f5f5;color:#666 !important;}
.btn_darkgray{background:#808080;color:#fff !important;}
.btn_mint{background:#2095ec;color:#fff !important;}
.btn_orange{background:#ff944d;color:#fff !important;}
.btn_green{background: #2ac44b;color:#fff !important;}
.btn_red{background: #ff3737;color:#fff !important;}
.btn .btn_sky > i, .btn_blue > i, .btn_mint > i, .btn_orange > i {padding-right:10px;}
.page_btn_num .trash_btn,.page_btn_num .setting_btn{position:absolute;top:35px;right:0;}
.page_btn_num .save_btn{position:absolute;right:140px;}
.btn input[type="button"]{border:none;}

/* checkbox */
.check input[type="checkbox"]{display:none;}
.check input[type="checkbox"] + label span{display:inline-block;width:16px;height:16px;margin:-4px 8px 0 0;background:url(../img/input_checkbox.png) 0 0 no-repeat;vertical-align:middle;}
.check input[type="checkbox"]:checked + label span{background-position: 0 -16px;}


/* radio */
.radio input[type="radio"]{display:none;}
.radio input[type="radio"] + label span{display:inline-block;width:16px;height:16px;margin:-5px 8px 0 0;background:url(../img/input_radio.png) 0 0 no-repeat;vertical-align:middle;}
.radio input[type="radio"]:checked + label span{background-position: 0 -16px;}


/* tab */
.tab{border-bottom:1px solid #33a0ee;box-sizing:border-box;margin-bottom:20px;}
.tab li{float:left;height:35px;line-height:35px;text-align:center;}
.tab li > a{display:block;}
.tab li > a.on,.tab li > a:hover{background:#33a0ee;border-radius:3px 3px 0 0; color:#fff;font-weight:500;}

/* select_box : 옵션부분 설정 */
.select_box{clear:both;position:relative;}
.option_list{position:relative;float:left;height:35px;line-height:35px;border: 1px solid #ddd;cursor:pointer;}
.option_list .select_tit{padding-left:10px;color:#333;}
.option_list .select_tit + ul{position:absolute;top:36px;left:-1px;z-index:50;}
.option_list li{position:relative;border-left: 1px solid #ddd;border-right: 1px solid #ddd;color:#333;}
.option_list li:not(.select_tit){background:#fff;}
.option_list li.last{border-bottom: 1px solid #ddd;}
.option_list li > a {display:block;width:100%;}
.option_list li > a > span{padding-left:10px;}
.option_list li > a:hover{font-weight:500;background:#f5f5f5;}

/*  테이블 종류 */

/* 페이징 있는 테이블 (basic_tbl_type) */
.collection .cont1.list{height:auto;overflow-y:hidden !important;}
.basic_tbl_type{width:100%;}
.basic_tbl_type thead{border-top: 2px solid #0054b3;  border-bottom: 1px solid #ddd;background:#f4f4f4;}
.basic_tbl_type thead th{height:40px;color:#333;}
.basic_tbl_type td{text-align:center;border-bottom: 1px solid #ddd; padding:5px 5px;word-break: break-all;}
.bule_tbl_type{width:100%;}
.bule_tbl_type thead{border-top: 2px solid #0054b3;  border-bottom: 1px solid #ddd;background:#f4f4f4;}
.bule_tbl_type thead th{height:44px;color:#333;}
.bule_tbl_type td{text-align:center;border-bottom: 1px solid #ddd; padding:10px 5px;word-break: break-all;}
.tableRowClickEffect1{background-color:#fffaeb !important;}
.tableRowClickEffect2{background-color:#fffaeb !important;}
.tableRowClickEffect3{background-color:#fffaeb !important;}
td.icon{font-size:18px;color: #888;}


/* 가로세로 테이블 (tbl_type_scroll)*/
.tbl_type_scroll {position:relative;border-collapse: collapse; width:1678px; overflow-x: scroll; display: block; border-top: 2px solid #0054b3;  border-bottom: 1px solid #ddd;}
.tbl_type_scroll thead {background-color: #f4f4f4;}
.tbl_type_scroll thead, .tbl_type_scroll tbody {display: block;}
.tbl_type_scroll tbody {overflow-y: scroll; overflow-x: hidden; height:200px;}
.tbl_type_scroll th {min-width:120px; height: 25px; overflow:hidden; text-overflow: ellipsis; max-width: 100px; text-align:center;    color: #333;}
.tbl_type_scroll td{min-width:110px; height:25px; text-align:center; border-bottom: 1px solid #ddd; padding:10px 5px;}
.tbl_type_scroll .tooltip{position:relative}
.tbl_type_scroll .tooltip .txt{max-width:110px; white-space: nowrap;  text-overflow: ellipsis; overflow:hidden; }
.tbl_type_scroll .tooltip:hover{text-decoration: underline;}
.tbl_type_scroll .tooltip .tooltip_wrap{position:absolute;z-index:50;display:block;background:#fff;border:1px solid #999;padding:10px 20px;width:160px; white-space:normal; color:#333; box-shadow: 2px 2px 2px -2px #d4d4d4; top:32px}       
.tbl_type_scroll .tooltip .tooltip_wrap:after{position:absolute;top:-12px;content: "";display: block;width:16px;height:12px;background:url(../img/bg_tooltip_accordion.png) 0 0 no-repeat;}


/* 세로 스크롤 테이블 (tbl_type_scroll.overflow_y)*/
.cont_box .overflow_y{position:relative;border-collapse: collapse; width:1688px;display: block; border-top: 2px solid #0054b3; overflow-x: scroll; border-bottom: 1px solid #ddd;overflow: hidden;-ms-overflow-x:hidden;}
.cont_box .overflow_y thead {background-color: #f4f4f4;}
.cont_box .overflow_y thead, .cont_box .overflow_y tbody {display: block;}
.cont_box .overflow_y tbody {overflow-y:auto; overflow-x: hidden; height:260px;}
.cont_box .overflow_y th {height:45px; overflow:hidden; text-overflow: ellipsis;text-align:center;    color: #333;}
.cont_box .overflow_y td{text-align:center; border-bottom: 1px solid #ddd; padding:10px 8px;word-break: break-all;}


/* page_btn_num */
.page_btn_num{position:relative;height:35px;color:#999;text-align:center;padding:35px 0;}
.page_btn_num a:hover{color:#0054b3;}
.page_btn_num .num{box-sizing:border-box;display:inline-block;width:35px;height:35px;line-height:35px; color:#999;border-radius: 3px;border: 1px solid #ddd;margin-left: 5px;}
.page_btn_num .right{box-sizing:border-box;display:inline-block;width:35px;height:35px;line-height:35px;color:#666;border-radius: 3px;border: 1px solid #ddd;margin-left: 5px;}
.page_btn_num .num.click{box-sizing:border-box;background-color:#fff;color:#0054b3;font-weight:bold; border:1px solid #0054b3;border-radius:3px;}
.btn_num .right.click{box-sizing:border-box;background-color:#fff;color:#0054b3;font-weight:bold; border:1px solid #0054b3;}


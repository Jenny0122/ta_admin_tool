/*  공통 cont_box css*/
.cont_tit{padding:20px 0 2.5px 0;}
.cont_tit h2{float:left;font-size:22px;color:#333;font-weight:500;}
.location{float:right;padding-top:5px;}
.location > li{display:inline-block;color:#999;}
.location > li > span{padding:0 7px;}
.location > li:last-child strong{font-weight:500;color:#666;}
.cont_box{background:#fff;border: 1px solid #e2e5e9;}
.cont_box h3{padding:15px 0 15px 25px;border-bottom:1px solid #ddd;font-size:18px;color:#333;font-weight:500;}
.container{padding:20px 25px;}
.container .tit{margin-bottom:15px;color:#0e3a6b;font-weight: 500;}
.container label{color: #333;font-weight:500;vertical-align: middle;}
.sub_tit{font-size:17px;color:#333;font-weight:500;}
.h3_tit_box{border-bottom:1px solid #ddd;}
.h3_tit_box h3{border-bottom:none;}
.h3_tit_box h3 + span{display:block;padding-top:20px;}

/* 조회날짜  */
.calendar{position:relative;}
.calendar > a{position:absolute;top:-3px;right:10px;font-size:20px;color:#888;}
.wave{padding:0 5px;}
.btn_sch{display:inline-block;vertical-align:middle; height:35px;line-height:33px;background:#e3e3e3;border-radius:3px;font-size:20px;text-align:center;}
input[type="date"]::-webkit-calendar-picker-indicator {color: transparent;background: none;z-index: 1;}
input[type="date"]:before {color: transparent;background: none;display: block;font-family: 'FontAwesome';content: '\f073';}

/* 실시간 업데이트 toggle_btn*/
.toggle_btn{position:relative;top:3px;display:inline-block;margin-left:15px;}
.tgl {display:none;}
.tgl + .tgl-btn {outline: 0;display: block;width:62px;height:30px;
 position: relative;cursor: pointer;}
.tgl, .tgl:after, .tgl:before {box-sizing: border-box;}
.tgl + .tgl-btn:after, .tgl + .tgl-btn:before {position: relative;
display: block;content: "";width:23px;height:23px;}
.tgl + .tgl-btn:after {top:3px;left:5px;}
.tgl + .tgl-btn:before {display: none;}
.tgl:checked + .tgl-btn:after {left: 50%;}
.tgl-light + .tgl-btn {background: #33a0ee;border-radius:30px;transition: all .4s ease;}
.tgl-light + .tgl-btn:after {border-radius: 15px;background: #fff;transition: all .2s ease;}
